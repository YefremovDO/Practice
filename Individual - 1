#include <iostream>
using namespace std;

int main()
{
    setlocale(LC_ALL, "Ukr");
    int k = 0, s = 0, n;
    cout << "Введіть размір масиву: ";
    cin >> n;
    int a[55];
    for (int i = 0; i < n; i++)
    cin >> a[i];
    cout << "\nВведіть K: ";
    cin >> k;
    cout << "\nЧисла крaтні K:  ";
    for (int i = 0; i < n; i++) 
    if (a[i] % k == 0)
    {
        cout << a[i]<< "\t";
        s += a[i];
    }
    cout << "\nСума чисіл кратних " << k << " = " << s << endl;
    system("PAUSE");
}
