#include<iostream>
#include <string>
#include "windows.h" 
using namespace std;

int main()
{
	SetConsoleCP(1251);
	SetConsoleOutputCP(1251);

	cout << "Введіть рядок: " << endl;
	string str;
	cin >> str;

	int num = stoi(str);

	cout << "\nОтримане число: " << endl;
	cout << num << endl;

	cout << "\nОтриманий рядок: " << endl;
	string str1 = to_string(num);
	cout << str1 << endl;

	return 0;
}
