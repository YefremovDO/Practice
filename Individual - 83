#include <iostream>
#include <time.h> 
using namespace std;

int main()
{
	int n;
	cout << "Enter n:" << endl;
	cin >> n;
	cout << "\n";
	int a = 0;

	for (int i = 0; i < n; i++)
	{
		if (a == 0)
		{
			a = n;
		}

		for (int j = 1; j <= n; j++)
		{
			if (i % 2 == 0)
			{
				cout << j << "	";
			}
			else
			{
				cout << a << "	";
				a = a - 1;
			}
		}
		cout << endl;
	}

	return 0;
}
