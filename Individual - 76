#include <iostream>
#include <cstdlib>
using namespace std;

int main()
{
	int arr[100];
	srand(time(NULL));
	int n;
	cout << "n = " << endl;
	cin >> n;
	cout << "before:"<< endl;
	for (int i = 0; i < n; i++)
	{
		arr[i] = rand() % 33;
		cout << arr[i] << "\t";
	}
	cout  << endl;
	
	for (int i = 0; i < n; i++)
	{
		for (int j = 0; j < (n - 1); j++)
		{
			if (arr[j] > arr[j + 1])
			{
				swap(arr[j], arr[j + 1]);
			} 
		}
	}
	cout << "after:" << endl;
	for (int i = 0; i < n; i++)
	{
		cout << arr[i] << "\t";
	}
	return 0;
}
